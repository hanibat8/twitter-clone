{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "strictNullChecks": false,
    "strict": false
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/Search.js",
    "components/withDataListHOC.tsx",
    "components/Posts/PostsList.tsx",
    "components/Posts/PostRetweetPostsList.tsx",
    "components/Posts/postsSlice.ts"
, "pages/_document.js"  ],
  "exclude": [
    "node_modules"
  ]
}
